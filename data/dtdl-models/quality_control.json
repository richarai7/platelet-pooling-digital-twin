{
  "@context": "dtmi:dtdl:context;3",
  "@id": "dtmi:platelet:device:QualityControl;1",
  "@type": "Interface",
  "displayName": "Quality Control",
  "description": "A QC testing station that performs automated quality tests on pooled platelet products.",
  "comment": "Simulates a quality control station in the platelet pooling process",
  "contents": [
    {
      "@type": "Property",
      "name": "deviceId",
      "displayName": "Device ID",
      "description": "Unique identifier for this device instance",
      "schema": "string",
      "writable": false
    },
    {
      "@type": "Property",
      "name": "deviceType",
      "displayName": "Device Type",
      "schema": "string",
      "writable": false
    },
    {
      "@type": "Property",
      "name": "state",
      "displayName": "Device State",
      "description": "Current operational state of the device",
      "schema": {
        "@type": "Enum",
        "valueSchema": "string",
        "enumValues": [
          {
            "name": "idle",
            "enumValue": "idle",
            "displayName": "Idle"
          },
          {
            "name": "processing",
            "enumValue": "processing",
            "displayName": "Processing"
          },
          {
            "name": "error",
            "enumValue": "error",
            "displayName": "Error"
          },
          {
            "name": "maintenance",
            "enumValue": "maintenance",
            "displayName": "Maintenance"
          }
        ]
      },
      "writable": true
    },
    {
      "@type": "Property",
      "name": "isProcessing",
      "displayName": "Is Processing",
      "description": "Whether the device is currently processing a batch",
      "schema": "boolean",
      "writable": true
    },
    {
      "@type": "Property",
      "name": "currentBatchId",
      "displayName": "Current Batch ID",
      "description": "ID of the batch currently being processed",
      "schema": "string",
      "writable": true
    },
    {
      "@type": "Property",
      "name": "errorState",
      "displayName": "Error State",
      "description": "Description of current error, if any",
      "schema": "string",
      "writable": true
    },
    {
      "@type": "Telemetry",
      "name": "test_temperature_celsius",
      "displayName": "Test Temperature",
      "description": "Current test temperature",
      "schema": "double",
      "unit": "degreeCelsius"
    },
    {
      "@type": "Property",
      "name": "test_temperature_celsius",
      "displayName": "Current Test Temperature",
      "description": "Current test temperature (as property for twin state)",
      "schema": "double",
      "writable": true
    },
    {
      "@type": "Telemetry",
      "name": "sample_volume_ml",
      "displayName": "Sample Volume",
      "description": "Current sample volume in mL",
      "schema": "double"
    },
    {
      "@type": "Property",
      "name": "sample_volume_ml",
      "displayName": "Current Sample Volume",
      "description": "Current sample volume in mL (as property for twin state)",
      "schema": "double",
      "writable": true
    },
    {
      "@type": "Telemetry",
      "name": "platelet_count_x10_9_per_L",
      "displayName": "Platelet Count",
      "description": "Platelet count x10^9/L",
      "schema": "double"
    },
    {
      "@type": "Property",
      "name": "platelet_count_x10_9_per_L",
      "displayName": "Current Platelet Count",
      "description": "Current platelet count x10^9/L (as property for twin state)",
      "schema": "double",
      "writable": true
    },
    {
      "@type": "Telemetry",
      "name": "ph_level",
      "displayName": "pH Level",
      "description": "Current pH level",
      "schema": "double"
    },
    {
      "@type": "Property",
      "name": "ph_level",
      "displayName": "Current pH Level",
      "description": "Current pH level (as property for twin state)",
      "schema": "double",
      "writable": true
    },
    {
      "@type": "Telemetry",
      "name": "glucose_level_mg_per_dL",
      "displayName": "Glucose Level",
      "description": "Glucose level in mg/dL",
      "schema": "double"
    },
    {
      "@type": "Property",
      "name": "glucose_level_mg_per_dL",
      "displayName": "Current Glucose Level",
      "description": "Current glucose level in mg/dL (as property for twin state)",
      "schema": "double",
      "writable": true
    },
    {
      "@type": "Telemetry",
      "name": "bacterial_test_result",
      "displayName": "Bacterial Test Result",
      "description": "Bacterial test result (pending, negative, positive)",
      "schema": "string"
    },
    {
      "@type": "Property",
      "name": "bacterial_test_result",
      "displayName": "Current Bacterial Test Result",
      "description": "Current bacterial test result (as property for twin state)",
      "schema": "string",
      "writable": true
    },
    {
      "@type": "Property",
      "name": "remaining_time_seconds",
      "displayName": "Remaining Time",
      "description": "Seconds remaining in current test",
      "schema": "integer",
      "writable": true
    },
    {
      "@type": "Property",
      "name": "tests_completed",
      "displayName": "Tests Completed",
      "description": "Total number of tests completed",
      "schema": "integer",
      "writable": false
    },
    {
      "@type": "Property",
      "name": "tests_passed",
      "displayName": "Tests Passed",
      "description": "Total number of tests passed",
      "schema": "integer",
      "writable": false
    },
    {
      "@type": "Property",
      "name": "tests_failed",
      "displayName": "Tests Failed",
      "description": "Total number of tests failed",
      "schema": "integer",
      "writable": false
    },
    {
      "@type": "Telemetry",
      "name": "pass_rate",
      "displayName": "Pass Rate",
      "description": "Test pass rate percentage",
      "schema": "double"
    },
    {
      "@type": "Property",
      "name": "pass_rate",
      "displayName": "Current Pass Rate",
      "description": "Current test pass rate percentage (as property for twin state)",
      "schema": "double",
      "writable": true
    },
    {
      "@type": "Property",
      "name": "total_runtime_hours",
      "displayName": "Total Runtime Hours",
      "description": "Total operational runtime in hours",
      "schema": "double",
      "writable": false
    },
    {
      "@type": "Property",
      "name": "lastTelemetryTime",
      "displayName": "Last Telemetry Time",
      "description": "Timestamp of last telemetry update",
      "schema": "dateTime",
      "writable": true
    },
    {
      "@type": "Telemetry",
      "name": "processingStarted",
      "displayName": "Processing Started Event",
      "schema": {
        "@type": "Object",
        "fields": [
          {
            "name": "eventType",
            "schema": "string"
          },
          {
            "name": "batchId",
            "schema": "string"
          },
          {
            "name": "timestamp",
            "schema": "dateTime"
          }
        ]
      }
    },
    {
      "@type": "Telemetry",
      "name": "processingComplete",
      "displayName": "Processing Complete Event",
      "schema": {
        "@type": "Object",
        "fields": [
          {
            "name": "eventType",
            "schema": "string"
          },
          {
            "name": "batchId",
            "schema": "string"
          },
          {
            "name": "testTimeMinutes",
            "schema": "double"
          },
          {
            "name": "success",
            "schema": "boolean"
          },
          {
            "name": "qualityMetrics",
            "schema": {
              "@type": "Object",
              "fields": [
                {
                  "name": "overallQualityScore",
                  "schema": "double"
                },
                {
                  "name": "plateletViability",
                  "schema": "double"
                },
                {
                  "name": "sterilityConfirmed",
                  "schema": "boolean"
                }
              ]
            }
          },
          {
            "name": "timestamp",
            "schema": "dateTime"
          }
        ]
      }
    },
    {
      "@type": "Telemetry",
      "name": "deviceError",
      "displayName": "Device Error Event",
      "schema": {
        "@type": "Object",
        "fields": [
          {
            "name": "eventType",
            "schema": "string"
          },
          {
            "name": "errorState",
            "schema": "string"
          },
          {
            "name": "timestamp",
            "schema": "dateTime"
          }
        ]
      }
    },
    {
      "@type": "Command",
      "name": "startProcessing",
      "displayName": "Start Processing",
      "description": "Start processing a new batch",
      "request": {
        "name": "batchId",
        "schema": "string"
      },
      "response": {
        "name": "success",
        "schema": "boolean"
      }
    },
    {
      "@type": "Command",
      "name": "stopProcessing",
      "displayName": "Stop Processing",
      "description": "Stop the current processing operation",
      "response": {
        "name": "success",
        "schema": "boolean"
      }
    },
    {
      "@type": "Command",
      "name": "clearError",
      "displayName": "Clear Error",
      "description": "Clear the current error state",
      "response": {
        "name": "success",
        "schema": "boolean"
      }
    }
  ]
}
